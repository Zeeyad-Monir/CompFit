{
  "expo": {
    "name": "workoutApp",
    "slug": "workoutApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "owner": "your-expo-username",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.zeeyad.compfit",
      "supportsTablet": true,
      "infoPlist": {
        "NSUserNotificationsUsageDescription": "This app uses notifications to alert you when you receive friend requests.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you attach photos to your workout submissions."
      }
    },
    "android": {
      "package": "com.zeeyad.compfit",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["NOTIFICATIONS", "READ_EXTERNAL_STORAGE"]
    },
    "plugins": [
      "expo-notifications",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you attach them to workout submissions."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}